	async Task NavLinkOnClick(string normalTitle)
	{
		MainMenuItem activeMenuItem = mainMenuItemList.Where(p => p.MenuItemList.Any(o => o.NormalTitle == normalTitle)).FirstOrDefault();
		
		DashboardActiveClass = (activeMenuItem?.Name == "Dashboards") ? "active" : "";
		PagesActiveClass = (activeMenuItem?.Name == "Pages") ? "active" : "";
		ApplicationActiveClass = (activeMenuItem?.Name == "Applications") ? "active" : "";
		EcommerceActiveClass = (activeMenuItem?.Name == "Ecommerce") ? "active" : "";
		AuthenticationActiveClass = (activeMenuItem?.Name == "Authentication") ? "active" : "";

		AppState.SetNavBarSelectedMainMenu(activeMenuItem.Name);
		AppState.SetNavBarSelectedMenu(normalTitle);

	}

	protected override void OnInitialized()
	{
		AppState.OnChange += StateHasChanged;
	}

	public void Dispose()
